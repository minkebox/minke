      },
      dns: false,
      mdns: null
    },
    {
      type: `Port`,
      name: `32400/udp`,
      port: 32400,
      protocol: `UDP`,
      nat: false,
      web: false,
      dns: false,
      mdns: null
    },
    {
      type: `Port`,
      name: `32469/tcp`,
      port: 32469,
      protocol: `TCP`,
      nat: false,
      web: false,
      dns: false,
      mdns: null
    },
    {
      type: `Port`,
      name: `32469/udp`,
      port: 32469,
      protocol: `UDP`,
      nat: false,
      web: false,
      dns: false,
      mdns: null
    },
    {
      type: `Port`,
      name: `5353/udp`,
      port: 5353,
      protocol: `UDP`,
      nat: false,
      web: false,
      dns: false,
      mdns: null
    },
    {
      type: `Network`,
      name: `primary`,
      defaultValue: `home`
    }
  ]
