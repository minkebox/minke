    {
      type: `Environment`,
      name: `SPAWN_ANIMALS`,
      defaultValue: true
    },
    {
      type: `Environment`,
      name: `SPAWN_MONSTERS`,
      defaultValue: true
    },
    {
      type: `Environment`,
      name: `SPAWN_NPCS`,
      defaultValue: true
    },
    {
      type: `Environment`,
      name: `VIEW_DISTANCE`,
      defaultValue: 10
    },
    {
      type: `Environment`,
      name: `SEED`
    },
    {
      type: `Environment`,
      name: `MOTD`,
      defaultValue: `Welcome to Minecraft on MinkeBox`
    },
    {
      type: `Environment`,
      name: `PVP`,
      defaultValue: false
    },
    {
      type: `Environment`,
      name: `LEVEL_TYPE`,
      defaultValue: `DEFAULT`
    },
    {
      type: `Environment`,
      name: `GENERATOR_SETTINGS`
    },
    {
      type: `Environment`,
      name: `LEVEL`,
      defaultValue: `world`
    },
    {
      type: `Environment`,
      name: `VERSION`,
      defaultValue: `LATEST`
    },
    {
      type: `Environment`,
      name: `TYPE`,
      defaultValue: `VANILLA`
    },
    {
      type: `Directory`,
      name: `/config`,
      backup: true
    },
    {
      type: `Directory`,
      name: `/data`,
      shares: [
        {
          name: `/`,
          description: `All the server configuration`
        },
        {
          name: `/config`,
          description: `Configure Mods here`
        },
        {
          name: `/mods`,
          description: `Install Mods here`
        },
        {
          name: `/plugins`,
          description: `Install Plugins here`
        }
      ],
      backup: true
    },
    {
      type: `Directory`,
      name: `/mods`,
      backup: true
    },
    {
      type: `Port`,
      name: `SERVER_PORT`,
      port: `{{SERVER_PORT}}`,
      protocol: `TCP`,
      nat: `{{PUBLIC_SERVER}}`,
      web: false,
      dns: false,
      mdns: null
    },
    {
      type: `Port`,
      name: `25575/tcp`,
      port: 25575,
      protocol: `TCP`,
      nat: false,
      web: false,
      dns: false,
      mdns: null
    },
    {
      type: `Network`,
      name: `primary`,
      defaultValue: `home`
    }
  ],
  monitor: {
    cmd: `mcstatus localhost status`,
    polling: 60,
    parser: `
      const lines = input.split('\\n');
      const nrplayers = lines[2].split(' ',2)[1].split('/');
      output.nr = nrplayers[0];
      output.max = nrplayers[1];
      try {
        output.players = eval(lines[2].replace(/^.*(\\[.*\\]).*$/, '$1')).map(p => p.split(' ')[0]);
      }
      catch (_) {
        output.players = [];
      }
    `,
    header: `
      <style>
        .mc-o { display: inline-block; width: 200px; padding-top: 20px; float: right; }
        .mc-b { color: grey; text-align: right; padding: 0 5px 10px 0; }
        .mc-r { display: inline-block; font-size: 18px; color: black; }
        .mc-p { font-size: 75%; padding-left: 4px; }
      </style>
    `,
    template: `
      <div class="mc-o">
        <div class="mc-b"><div class="mc-r">{{max}}</div><div>Max Players</div></div>
        <div class="mc-b"><div class="mc-r">{{nr}}</div><div>Current</div></div>
        <div class="mc-b"><div class="mc-r">
          {{#each players}}<span class="mc-p">{{this}}</span>{{/each}}
        </div><div>Online players</div></div>
      </div>
    `
  }
}
