      description: 'Mod type'
    },
    {
      type: `Header`,
      title: `Advanced`,
      enabled: 'property.FirstUse'
    },
    {
      type: `EditEnvironment`,
      name: `VERSION`,
      description: `Specific Minecraft version`
    },
    {
      type: `EditEnvironment`,
      name: `SEED`,
      description: `Seed`
    },
    {
      type: `EditEnvironment`,
      name: `LEVEL_TYPE`,
      options: [
        {
          name: `Default`,
          value: `DEFAULT`
        },
        {
          name: `Flat`,
          value: `FLAT`
        },
        {
          name: `Large biomes`,
          value: `LARGEBIOMES`
        },
        {
          name: `Amplified`,
          value: `AMPLIFIED`
        },
        {
          name: `Customized`,
          value: `CUSTOMIZED`
        },
        {
          name: `Buffer`,
          value: `BUFFET`
        }
      ],
      description: `Level type`
    },
    {
      type: `Header`,
      title: `Generator`,
      visible: `property['Environment#LEVEL_TYPE'] === 'FLAT' || property['Environment#LEVEL_TYPE'] === 'CUSTOMIZED' || property['Environment#LEVEL_TYPE'] === 'BUFFET'`,
      enabled: 'property.FirstUse'
    },
    {
      type: `EditEnvironment`,
      name: `GENERATOR_SETTINGS`,
