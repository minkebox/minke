      defaultValue: false
    },
    {
      type: `Environment`,
      name: `ANNOUNCE_PLAYER_ACHIEVEMENTS`,
      defaultValue: true
    },
    {
      type: `Environment`,
      name: `ENABLE_COMMAND_BLOCK`,
      defaultValue: false
    },
    {
      type: `Environment`,
      name: `GENERATE_STRUCTURES`,
      defaultValue: true
    },
    {
      type: `Environment`,
      name: `HARDCORE`,
      defaultValue: false
    },
    {
      type: `Environment`,
      name: `SNOOPER_ENABLED`,
      defaultValue: false
    },
    {
